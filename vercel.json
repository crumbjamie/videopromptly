{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "installCommand": "npm install",
  "regions": ["syd1"],
  "functions": {
    "app/api/prompts/route.ts": {
      "maxDuration": 10
    },
    "app/api/search/route.ts": {
      "maxDuration": 10
    }
  },
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  },
  "github": {
    "enabled": true,
    "autoAlias": true
  },
  "outputFileTracing": {
    "ignore": [
      "node_modules/sharp/**/*",
      "scripts/**/*",
      "*.md",
      "*.backup.json",
      "public/thumbnails/square/**/*"
    ]
  }
}