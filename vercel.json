{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "installCommand": "npm install",
  "regions": ["syd1"],
  "functions": {
    "app/api/prompts/route.ts": {
      "maxDuration": 10
    },
    "app/api/search/route.ts": {
      "maxDuration": 10
    }
  },
  "git": {
    "deploymentEnabled": {
      "main": true,
      "preview": false
    }
  },
  "github": {
    "enabled": true,
    "autoAlias": true
  }
}